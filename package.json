{
	"name": "gcs-bucket-cdktf",
	"version": "1.0.0",
	"main": "src/main.ts",
	"types": "src/main.ts",
	"license": "MPL-2.0",
	"private": true,
	"scripts": {
		"get": "cdktf get",
		"build": "tsc",
		"synth": "cdktf synth",
		"deploy": "cdktf deploy",
		"compile": "tsc --pretty",
		"watch": "tsc -w",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"upgrade": "yarn upgrade cdktf@latest cdktf-cli@latest",
		"upgrade:next": "yarn upgrade cdktf@next cdktf-cli@next"
	},
	"engines": {
		"node": ">=18.0"
	},
	"dependencies": {
		"@cdktf/provider-google": "7.x",
		"cdktf": "0.16.x",
		"cdktf-cli": "0.16.x",
		"constructs": "^10.2.5",
		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@swc/core": "^1.3.34",
		"@swc/jest": "^0.2.26",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.11.19",
		"jest": "^29.4.2",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"tsx": "^3.12.6",
		"typescript": "^4.9.5"
	},
	"jest": {
		"transform": {
			"^.+\\.(t|j)sx?$": [
				"@swc/jest"
			]
		},
		"setupFilesAfterEnv": [
			"<rootDir>/src/setup_test.ts"
		]
	}
}
